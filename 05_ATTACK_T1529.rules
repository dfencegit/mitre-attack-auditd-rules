# -----------------------------------------------------------------------
# ATTACK_T1529 - System Shutdown/Reboot
# -----------------------------------------------------------------------
-w /sbin/shutdown -p x -k ATTACK_T1529
-w /sbin/poweroff -p x -k ATTACK_T1529
-w /sbin/reboot -p x -k ATTACK_T1529
-w /sbin/halt -p x -k ATTACK_T1529
