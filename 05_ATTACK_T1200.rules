# -----------------------------------------------------------------------
# ATTACK_T1200 - Hardware Additions
# -----------------------------------------------------------------------
-a always,exit -F arch=b64 -S mount -S umount2 -F auid!=-1 -k ATTACK_T1200
-a always,exit -F arch=b32 -S mount -S umount -S umount2 -F auid!=-1 -k ATTACK_T1200
-a always,exit -F arch=b64 -S swapon -S swapoff -F auid!=-1 -k ATTACK_T1200
-a always,exit -F arch=b32 -S swapon -S swapoff -F auid!=-1 -k ATTACK_T1200
