# ATTACK_T1055_008 - Ptrace System Calls
-a exit,always -F arch=b32 -S ptrace -k ATTACK_T1055_008
-a exit,always -F arch=b64 -S ptrace -k ATTACK_T1055_008
-a always,exit -F arch=b32 -S ptrace -F a0=0x4 -k ATTACK_T1055_008
-a always,exit -F arch=b64 -S ptrace -F a0=0x4 -k ATTACK_T1055_008
-a always,exit -F arch=b32 -S ptrace -F a0=0x5 -k ATTACK_T1055_008
-a always,exit -F arch=b64 -S ptrace -F a0=0x5 -k ATTACK_T1055_008
-a always,exit -F arch=b32 -S ptrace -F a0=0x6 -k ATTACK_T1055_008
-a always,exit -F arch=b64 -S ptrace -F a0=0x6 -k ATTACK_T1055_008
